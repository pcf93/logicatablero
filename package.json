{
  "name": "cordelia",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-only": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore --fix src && prettier -w .",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "test": "vitest"
